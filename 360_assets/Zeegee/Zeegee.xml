  <hotspots>
    <hotspot id="zaxismicrofeeds" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Main Drop Rate Micro Feed&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;Tandem Z axis adjustment enables accurate setting of the main tilt/roll drop rate and can be easily modified on the fly. " css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="tiltfluiddragandbrake" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Tilt Axis Fluid Drag and Brake&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;The tilt axis incorporates a variable fluid drag adjustment that smooths out any oscillation that is more likely to occur with shorter camera builds. Fully tightening down serves as a tilt brake. " css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="slidingcamerastage" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" disabled="true" activateOnClick="true">
      <spotinfo />
    </hotspot>
    <hotspot id="rollaxisdroprate" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Independent Roll Drop Rate&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;Adjusts the roll axis drop rate independent of the tilt axis." css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="panbrake" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Pan Brake&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;Locks off the pan axis relative to the post socket for lock off shots on the balancing bar or operation off any 5/8” baby pin. " css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="orientableandinterchangablehandles" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Interchangeable Handles&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;ZeeGee comes with both a low profile knobs as well as handles that can quickly be swapped out." css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="inversionmodelock" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Inverted Operation Lock for Docking&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;Locks the cradle / pan base assembly in the upward facing inverted position when unit is docked in the handle yolks for underslung connection. " css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
    <hotspot id="candlerolllock" renderMode="0" indicatorImage="circ-cross-thin-grey.svg" activateOnClick="true">
      <spotinfo txt="&lt;b&gt;Cradle Roll Lock&lt;/b&gt;&#xD;&#xA;&#xD;&#xA;Locks the role axis in the center position for when mounting on the balancing bar or operation off any 5/8” baby pin. " css="font-family:Georgia;font-size:12px;color:#333333;width:200px;padding:15px 15px;border:1px solid #EEEEEE;background-color:rgba(255,255,255,1);" />
    </hotspot>
